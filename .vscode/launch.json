    "version": "0.2.0",
    "configurations": [
        {
            "name": "Playback",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/source",
            "module": "playback"
        },
        {
            "name": "Record",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/source",
            "module": "record"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/source",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}